package com.srinu.classes;

public class SecondClassEmployee {

	int empid;
	String name;
	static String compname = "w3s";

	public SecondClassEmployee(int empid, String name) {
		super();
		this.empid = empid;
		this.name = name;
	}

	// 0 param condstructor
	SecondClassEmployee() {
		System.out.println("o param default constructo");
	}

	public void dispaly() {
		System.out.println("emp id is::" + empid);
		System.out.println("name is:" + name);
		System.out.println("comp name is:" + compname);
	}

	public static void main(String[] args) {

		SecondClassEmployee e = new SecondClassEmployee();
		SecondClassEmployee e1 = new SecondClassEmployee(101, "srinivas");

		e1.dispaly();

		int i = 10;
		String j = Integer.toBinaryString(i);
		

		System.out.println(j);
	}

}

package comsrinu.methods;

public class MethodsCalling {

	int i = 6;

	public void test(MethodsCalling object) {

		int a = object.i++;
		System.out.println(a);
		System.out.println(i);
	}

	public static void main(String[] args) {
		MethodsCalling m = new MethodsCalling();

		m.test(m);

	}

}
